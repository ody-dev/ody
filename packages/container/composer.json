{
  "name": "ody/container",
  "description": "dependency injection container for ODY framework",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyas Deckers",
      "email": "ilyas@ody.dev"
    }
  ],
  "require": {
    "php": ">=8.3",
    "psr/container": "^1.1.2",
    "psr/simple-cache": "^1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Ody\\Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ody\\Container\\Tests\\": "tests/"
    }
  },
  "version": "0.1.2"
}
