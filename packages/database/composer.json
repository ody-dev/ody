{
  "name": "ody/database",
  "description": "Database package for ody API framework",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyas Deckers",
      "email": "ilyas@ody.dev"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-pdo": "*",
    "ext-swoole": "^6.0",
    "illuminate/database": "^11.43",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "ramsey/uuid": "^3.7|^4.0"
  },
  "autoload": {
    "psr-4": {
        "Ody\\DB\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ody\\DB\\Tests\\": "tests/"
    }
  },
  "extra": {
    "providers": [
      "Ody\\Database\\DatabseServiceProvider"
    ]
  }
}
