{
  "name": "ody/cqrs",
  "description": "CQRS module for ody API framework",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyas Deckers",
      "email": "ilyas@ody.dev"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-swoole": "^6.0",
    "ody/container": "^0.1.0",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "ramsey/uuid": "^3.7|^4.0",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Ody\\CQRS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ody\\CQRS\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test:unit": "phpunit --testsuite=Unit",
    "test:integration": "phpunit --testsuite=Integration",
    "test:coverage": "phpunit --coverage-html coverage-report"
  }
}
