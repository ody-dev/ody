{
  "name": "ody/framework",
  "description": "description",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyas Deckers",
      "email": "ilyas@ody.dev"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ody/ody-repo": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "symfony/var-dumper": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../"
    }
  ]
}
